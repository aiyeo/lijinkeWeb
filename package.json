{
  "name": "Jinke",
  "version": "1.0.0",
  "main": "index.js",
  "author": "jinke",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf public/static",
    "prod-server": "npm run build && cross-env NODE_ENV=PROD node server.js",
    "dev-server": "set debug=*,-not_this && cross-env NODE_ENV=DEV nodemon server.js",
    "build": "npm run clean && webpack --env.mode=PROD --progress --config webpack.config.js",
    "dev": "npm run clean &&  webpack-dev-server --config webpack.config.js",
    "upgrade": "yarn upgrade",
    "restore":"mongorestore -d lijinkeWeb --drop ./db/lijinkeWeb",
    "dump":"mongodump -d lijinkeWeb -o ./db"
  },
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "cors": "^2.8.3",
    "cross-env": "^4.0.0",
    "cssnano": "^3.10.0",
    "debug": "^2.6.6",
    "imagemin-webpack-plugin": "^1.4.4",
    "mongoose": "^4.9.9",
    "multiparty": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "react": "15.x",
    "react-dom": "15.x",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.0",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-server": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "6.x",
    "copy-webpack-plugin": "latest",
